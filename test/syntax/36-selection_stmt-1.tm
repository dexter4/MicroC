// ====================
// c startup
// ====================
0: ld gp, 0(0)
1: st 0, 0(0)
2: lda fp, -0(gp)
3: lda sp, -0(gp)
4: push fp
5: lda 0, 2(pc)
6: push 0
7: ldc pc, 9
8: halt
// ====================
// main()
// ====================
9: lda sp, -4(sp)
10: ld 0, -8(fp)
11: ldc 1, 0
12: gt 2, 0, 1
13: jle 2, 1(pc)
14: ldc 27, 0
15: lda sp, 0(fp)
16: ld fp, 0(fp)
17: ld pc, -4(sp)
// ====================
